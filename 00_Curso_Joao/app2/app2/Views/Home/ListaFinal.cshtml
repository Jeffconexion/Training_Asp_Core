@model IEnumerable<app2.Models.RespostaConvidados>
@{
    ViewData["Title"] = "ListaFinal";
}

<h2>ListaFinal</h2>
<hr />
<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>

            <td>@item.Nome</td>
            <td>@item.Email</td>
            <td>@item.Telefone</td>

        </tr>

        }
    </tbody>
</table>
